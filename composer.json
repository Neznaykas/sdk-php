{
    "name": "spiral/temporal",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "psr/log": "^1.1",
        "nesbot/carbon": "^2.41",
        "react/promise": "^2.8",
        "symfony/polyfill-php80": "^1.18",
        "spiral/roadrunner": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Temporal\\Client\\": "src"
        }
    },
    "require-dev": {
        "monolog/monolog": "^2.1",
        "doctrine/annotations": "^1.10",
        "react/socket": "^0.8",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.14",
        "symfony/var-dumper": "^5.1",
        "illuminate/support": "^7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "example",
            "Temporal\\Tests\\Client\\": "tests"
        }
    },
    "suggest": {
        "doctrine/annotations": "^1.0 for Doctrine metadata driver support"
    },
    "scripts": {
        "check": "psalm"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
